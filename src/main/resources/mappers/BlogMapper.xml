<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="me.stevenkin.blogspider.dao.BlogMapper">
    <insert id="insertBlog" parameterType="me.stevenkin.blogspider.enity.Blog">
        insert into blog_info(title,resume,link,site,tags) values(#{title},#{resume},#{link},#{site},#{tags})
    </insert>
</mapper>